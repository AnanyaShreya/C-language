/* Write a code to calculate the factorial of the number using function. */

#include<stdio.h>
int factorial(int);
int main()
{
	int num;
	printf("/nEnter any number: ");
	scanf("%d", &num);
	factorial(num);
}

int factorial(int num)
{
	int i,fact=1;
	for(i=1; i<=num; i++)
	{
		fact=fact*i;		
	}
	printf("\nFactorial of %d number is: %d",num,fact);
	return fact;
}
	
