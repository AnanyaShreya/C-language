/* Input Ten Number and Find Out Largest One.*/
#include<stdio.h>
int main()
{
	int n1,n2,n3,n4,n5,n6,n7,n8,n9,n10;
	printf("Enter number 1: ");
	scanf("%d", &n1);
	
	printf("Enter number 2: ");
	scanf("%d", &n2);
	
	printf("Enter number 3: ");
	scanf("%d", &n3);
	
	printf("Enter number 4: ");
	scanf("%d", &n4);
	
	printf("Enter number 5: ");
	scanf("%d", &n5);
	
	printf("Enter number 6: ");
	scanf("%d", &n6);
	
	printf("Enter number 7: ");
	scanf("%d", &n7);
	
	printf("Enter number 8: ");
	scanf("%d", &n8);
	
	printf("Enter number 9: ");
	scanf("%d", &n9);
	
	printf("Enter number 10: ");
	scanf("%d", &n10);
	
	if(n1>n2 && n1>n3 && n1>n4 && n1>n5 && n1>n6 && n1>n7 && n1>n8 && n1>n9 && n1>n10)
		printf(" %d is the greatest", n1);
		
	else if(n2>n1 && n2>n3 && n2>n4 && n2>n5 && n2>n6 && n2>n7 && n2>n8 && n2>n9 && n2>n10)
		printf(" %d is the greatest", n2);	

	else if(n3>n2 && n3>n1 && n3>n4 && n3>n5 && n3>n6 && n3>n7 && n3>n8 && n3>n9 && n3>n10)
		printf(" %d is the greatest", n3);

	else if(n4>n2 && n4>n3 && n4>n1 && n4>n5 && n4>n6 && n4>n7 && n4>n8 && n4>n9 && n4>n10)
		printf(" %d is the greatest", n4);

	else if(n5>n2 && n5>n3 && n5>n4 && n5>n1 && n5>n6 && n5>n7 && n5>n8 && n5>n9 && n5>n10)
		printf(" %d is the greatest", n5);

	else if(n6>n2 && n6>n3 && n6>n4 && n6>n5 && n1>n6 && n6>n7 && n6>n8 && n6>n9 && n6>n10)
		printf(" %d is the greatest", n6);

	else if(n7>n2 && n7>n3 && n7>n4 && n7>n5 && n7>n6 && n7>n1 && n7>n8 && n7>n9 && n7>n10)
		printf(" %d is the greatest", n7);

	else if(n8>n2 && n8>n3 && n8>n4 && n8>n5 && n8>n6 && n8>n7 && n8>n1 && n8>n9 && n8>n10)
		printf(" %d is the greatest", n8);

	else if(n9>n2 && n9>n3 && n9>n4 && n9>n5 && n9>n6 && n9>n7 && n9>n8 && n9>n1 && n9>n10)
		printf(" %d is the greatest", n9);

	else if(n10>n2 && n10>n3 && n10>n4 && n10>n5 && n10>n6 && n10>n7 && n10>n8 && n10>n9 && n10>n1)
		printf(" %d is the greatest", n10);		
	
	else 
		printf("You may have entered two same numbers");
	return 0;
}	
